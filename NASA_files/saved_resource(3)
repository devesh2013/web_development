;
jQuery( function() {
    //Calculate the Circumference of the mag spinner
    function magSliderCircumference(circle){
        let radius = circle.attr('r');
        return 2 * Math.PI * radius;
    }

    function magUpdateCircles(slider, slick, index, init = false){
        let circle = slider.parent().find('.hds-progress-ring circle');
        let msTotal = slick.options.autoplaySpeed + ( init ? 0 : slick.options.speed );

        //get the hds progress ring that is the same index as the current slide
        let progressRing = circle.eq(index);

        if(!slider.closest('.hds-carousel-wrapper').hasClass('paused')) {
            progressRing.show().css('transition', 'stroke-dashoffset ' + msTotal + 'ms linear').css('stroke-dashoffset', "0");
        }

        //hide all the other circles
        circle.not(progressRing).hide(function(){
            circle.not(progressRing).css('transition', 'none').css('stroke-dashoffset', magSliderCircumference(progressRing).toString());
        });
    }

    jQuery('.nasa-mag-slider').on('init', function(event, slick){
        magUpdateCircles(jQuery(this), slick, 0, true);
    });

    jQuery('.nasa-mag-slider').on('beforeChange', function(event, slick, currentSlide, nextSlide) {
        magUpdateCircles(jQuery(this), slick, nextSlide);
    });

    jQuery('.hds-mag-progress:not(.hds-mag-pause-play)').on('click', function(){
        let index = jQuery(this).index();
        jQuery(this).closest('.hds-carousel-wrapper').find('.nasa-mag-slider').slick('slickGoTo', index);
    });

    jQuery('.nasa-mag-slider').each(function() {
        jQuery(this).not('.slick-initialized').slick({
            infinite: true,
            dots: false,
            slidesToShow: 1,
            slidesToScroll: 1,
            autoplay: true,
            autoplaySpeed: 14000,
            speed: 1000,
            prevArrow: false,
            nextArrow: false,
            fade: true,
            pauseOnHover: false,
        });
    });

    jQuery('.hds-mag-pause-play').on('click', function(e){
        e.preventDefault();

        let wrapper = jQuery(this).closest('.hds-carousel-wrapper');
        let slider = wrapper.find('.nasa-mag-slider');

        //get the current slide
        let currentSlide = slider.slick('slickCurrentSlide');
        //get slick instance
        let slick = slider.slick('getSlick');


        if (wrapper.hasClass('paused')) {
            wrapper.removeClass('paused');
            slider.slick('slickPlay');
                    
            wrapper.find('.splide__toggle__pause').show();
            wrapper.find('.splide__toggle__play').hide();

            magUpdateCircles(slider, slick, currentSlide);

        } else {
            wrapper.addClass('paused');
            slider.slick('slickPause');
            wrapper.find('.splide__toggle__pause').hide();
            wrapper.find('.splide__toggle__play').show();

            wrapper.find('.hds-progress-ring circle').css('transition', 'none').css('stroke-dashoffset', magSliderCircumference(wrapper.find('.hds-progress-ring circle').eq(0)).toString());

        }

    });
});
;
//Run ECMAScript in strict mode
'use strict';

jQuery( function() {
    //Foreach image carousel slider in the page
    jQuery('.card-carousel-slider').each(function() {
        //Create the slider
        jQuery(this).not('.slick-initialized').slick({
            infinite: false,
            dots: false,
            slidesToShow: 3,
            slidesToScroll: 1,
            autoplay: false,
            autoplaySpeed: 5000,
            //link the next and previous buttons to the carousel
            prevArrow: jQuery(this).closest('.hds-card-carousel').find('.hds-carousel-arrow-prev'),
            nextArrow: jQuery(this).closest('.hds-card-carousel').find('.hds-carousel-arrow-next'),
            responsive: [
                {
                  breakpoint: 1200,
                  settings: {
                    slidesToShow: 2,
                    slidesToScroll: 1,
                  },
                },
                {
                  breakpoint: 800,
                  settings: {
                    slidesToShow: 1,
                    slidesToScroll: 1,
                  }
                },
            ],
        });
    });
});
;
